{
	"schemaVersion": 1,
	"id": "spellengineextension",
	"version": "${version}",
	"name": "Spell Engine Extension",
	"description": "Extends the mod 'Spell Engine' with additional functionality.",
	"authors": [
		"TheRedBrain"
	],
	"contact": {
		"sources": "https://github.com/TheRedBrain/spellengine-extension"
	},
	"license": "MIT",
	"icon": "assets/spellengineextension/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.github.theredbrain.spellengineextension.SpellEngineExtension"
		],
		"client": [
			"com.github.theredbrain.spellengineextension.SpellEngineExtensionClient"
		]
	},
	"mixins": [
		"spellengineextension.mixins.json"
	],
	"depends": {
		"minecraft": "${minecraft_version}",
		"fabricloader": ">=${loader_version}",
		"fabric-api": "*",
		"cloth-config": ">=${cloth_config_version}",
		"spell_engine": ">=${spell_engine_version}"
	},
	"suggests": {
		"manaattributes": "*",
		"staminaattributes": "*"
	}
}